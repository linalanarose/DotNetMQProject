<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="SenderDatabaseCommunication" type="System.Configuration.NameValueSectionHandler" />
    <section name="SQLiteDataBaseConfigure" type="System.Configuration.NameValueSectionHandler" />
  </configSections>
   <startup useLegacyV2RuntimeActivationPolicy="true">
        <supportedRuntime version="v2.0.50727"/></startup>
  <SenderDatabaseCommunication>
	 <add key="maxsize" value="30000" />
  </SenderDatabaseCommunication>
  <SQLiteDataBaseConfigure>
    <add key = "FilePath" value = "C:/SQLiteDataBase/MessageDatabase.sqlite" />
    <add key = "DirectoryPath" value = "C:/SQLiteDataBase/" />
  </SQLiteDataBaseConfigure>
</configuration>
